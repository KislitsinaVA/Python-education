summa = 0
count = 0
with open("input.txt", "r", encoding="utf-8") as f:
    for line in f.readlines():
        if len(line.strip()) == 0:
            continue
        sotr = line.split()
        if float(sotr[1]) < 20000:
            print(f"{sotr[0]} - Нищеброд")
        summa += float(sotr[1])
        count += 1
        midl_zp = round(summa / count, 2)
print(f"Средняя ЗП = {midl_zp}"